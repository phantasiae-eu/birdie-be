{
  "parser": "@typescript-eslint/parser",
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "extends": [
      "plugin:@typescript-eslint/recommended", "prettier/@typescript-eslint", "plugin:prettier/recommended", "prettier"
  ],
  "plugins":["@typescript-eslint", "prettier"],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  }
}